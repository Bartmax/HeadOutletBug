@page
@using Microsoft.AspNetCore.Components.Web
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    @*<title>This should not be rendered if specified.</title> *@
     <component type="typeof(HeadOutlet)" render-mode="WebAssemblyPrerendered" /> @* Ideally this should be ServerPrerendered ? *@
    <base href="/" />
    <link href="css/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="css/app.css" rel="stylesheet" />
    <link href="HeadOutletBug.Client.styles.css" rel="stylesheet" />
</head>

<body>
    
    <div id="app">
        <component type="typeof(HeadOutletBug.Client.App)" render-mode="WebAssemblyPrerendered" />
    </div>

    <div id="blazor-error-ui">
        An unhandled error has occurred.
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>
    <script src="_framework/blazor.webassembly.js"></script>
</body>

</html>
